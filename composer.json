{
    "name": "lig/models",
    "description": "ENACO Models",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.enaco.fr"
        },
        {
            "type": "composer",
            "url": "http://packages-private.enaco.fr"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/orm": "~2.0@stable",
        "sylius/addressing": "~0.1@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0@stable",
        "phpspec/phpspec": "~2.0@stable"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "Proprietary",
    "authors": [
        {
            "name": "LIG Developers",
            "email": "developers@enaco.fr"
        }
    ],
    "minimum-stability": "stable"
}

